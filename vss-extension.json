{
    "manifestVersion": 1,
    "id": "parapy-tools",
    "name": "ParaPy Cloud Tools",
    "version": "0.0.33",
    "publisher": "ParaPy",
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "categories": [
        "Azure Pipelines"
    ],
    "icons": {
        "default": "images/parapy.jpg"        
    },
    "branding": {
        "theme": "light"
    },
    "content": {
        "details": {
            "path": "README.md"
        }
    },
    "links": {
        "parapy": {
            "uri": "https://parapy.nl"
        }
    },
    "tags": ["__DoNotDownload"],
    "repository": {
        "type": "git",
        "uri": "https://github.com/parapydev/azure-pipeline-tasks"
    },
    "files": [
        {
            "path": "release-task"
        },
        {
            "path": "test-task"
        }
    ],
    "contributions": [
 {
            "id": "release-task",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "release-task"
            }
        },
 {
            "id": "test-task",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "test-task"
            }
        }
    ]
}